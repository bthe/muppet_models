              filter(year %in% 1995:tyr) %>% 
              filter(model == 'logit_length') %>% 
              select(year,
                     Catch = CalcCatchIn1000tons,
                     `Recruitment` = N3,
                     `F` = RefF,
                     `SSB` = Spawningstock) %>% 
              gather(variable,m,-year),col='red') +
  geom_vline(xintercept = 2012) +
  expand_limits(y=0) +
  facet_wrap(~variable,scale='free_y')+
  theme(strip.background = element_blank()) + 
  labs(y='',x='Year')
@


